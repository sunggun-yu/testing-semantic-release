{
    "branch": "master",
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "CHANGELOG.md",
                "changelogTitle": "# Semantic Versioning Changelog"
            }
        ],
        [
            "@semantic-release/exec",
            {
                "verifyConditionsCmd": "echo ${options.branch}",
                "prepareCmd": ".circleci/prepare-release.sh ${nextRelease.version}",
                "publishCmd": "echo ${nextRelease.version} ${options.branch} ${commits.length} ${Date.now()}",
                "successCmd": "echo releases - ${JSON.stringify(releases)} && echo nextRelease - ${JSON.stringify(nextRelease)}"
            }
        ],
        [
            "@semantic-release/git",
            {
                "assets": [
                    "CHANGELOG.md"
                ]
            }
        ],
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "dist/**"
                    }
                ]
            }
        ]
    ]
}
